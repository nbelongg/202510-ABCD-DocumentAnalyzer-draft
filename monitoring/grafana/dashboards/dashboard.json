{
  "dashboard": {
    "title": "Document Analyzer Metrics",
    "tags": ["document-analyzer"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}}"
          }
        ]
      },
      {
        "id": 2,
        "title": "Request Duration (p95)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{endpoint}}"
          }
        ]
      },
      {
        "id": 3,
        "title": "Requests In Progress",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "sum(http_requests_in_progress)"
          }
        ]
      },
      {
        "id": 4,
        "title": "LLM Calls",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 8},
        "targets": [
          {
            "expr": "sum(rate(llm_calls_total[5m]))"
          }
        ]
      },
      {
        "id": 5,
        "title": "Analysis Duration",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(document_analysis_duration_seconds_bucket[5m]))",
            "legendFormat": "{{document_type}}"
          }
        ]
      },
      {
        "id": 6,
        "title": "Database Operations",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "rate(database_operations_total[5m])",
            "legendFormat": "{{operation}} {{table}}"
          }
        ]
      },
      {
        "id": 7,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\"}[5m])",
            "legendFormat": "{{endpoint}}"
          }
        ]
      },
      {
        "id": 8,
        "title": "Token Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "rate(llm_tokens_total[5m])",
            "legendFormat": "{{provider}} {{model}} {{type}}"
          }
        ]
      }
    ],
    "schemaVersion": 30,
    "version": 1
  }
}
